{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "lerna run test --since ",
    "test-common": "lerna run test --scope=@gskTest/common",
    "test-deps": "lerna run test --scope={@gskTest/server,@gskTest/front}",
    "new-version": "lerna version --conventional-commits --yes",
    "diff": "lerna diff",
    "changed": "lerna changed --since",
    "list": "lerna list --since",
    "ci:build": "lerna run ci:build",
    "ci:zip": "lerna run zip",
    "build": "rimraf dist && cross-env NODE_ENV=production gulp build",
    "build:extensions": "cross-env-shell func extensions sync --prefix $npm_package_config_azureFunctions_outDir --output bin",
    "build:webpack": "rimraf dist && cross-env NODE_ENV=production webpack"
  },
  "name": "yarn-workspaces",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "dependencies": {
    "@lerna/changed": "^4.0.0"
  }
}
